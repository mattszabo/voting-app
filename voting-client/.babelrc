{
  "presets": [
    "es2015",
    "react"
  ],
  "env": {
    "start": {
      "presets": [
        "react-hmre"
      ],
      "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties",
        "syntax-trailing-function-commas"
      ]
    },
    "build": {
      "plugins": [
        "transform-react-constant-elements",
        "transform-object-rest-spread",
        "transform-class-properties",
        "syntax-trailing-function-commas"
      ]
    }
  }
}
